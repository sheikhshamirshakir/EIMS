@(discounts: List[models.Fees.DiscountCategory])
@import helper._

@mainlist("List of Faculties"){

 
  	@if(flash.get("msg")){
         	<span>@flash.get("msg")</span>
     }
  <!-- main content -->
  <div id="main_wrapper">
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-12">
          <table id="datatable_demo" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead> 
             <tr>
              <th>ID</th>
              <th>Discount Name</th>
              <th>Fees Head Name</th>
         
             </tr>
           </thead>
           <tbody>
             @for(discount <- discounts) {
             <tr>
               <td><a href="@routes.DiscountManagement.show(discount.id)">@discount.id</a></td>
               <td>@discount.name</td>
               <td>@discount.feesHead.name</td>
             </tr>
            }
          </tbody>
         </table>
       </div>
     </div>
   </div>
 </div>        
}      